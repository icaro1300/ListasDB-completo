@{
}

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col"> Nome</th>
            <th scope="col">Idade</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in @ViewBag.listaResponsavel)
        {
            <tr>
                <td>@item.RespID </td>
                <td>@item.RespNome </td>
                <td>@item.RespIdade </td>
            </tr>
        }
    </tbody>
</table>
